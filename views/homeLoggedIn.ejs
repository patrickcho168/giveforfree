	<link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">

	<!-- Google Analytics -->
	<script src="/scripts/common/analytics.js"></script>

    <!-- Facebook Like -->
    <script>
        $(window).bind("load", function() {
            $.getScript("/scripts/common/fblike.js", function() {});
        });
    </script>
    <!-- <script src="/scripts/common/fblike.js"></script> -->

</head>


<body>
	<div class="container-fluid">
		<!-- Facebook Like -->
	    <div id="fb-root"></div>
		<!-- Page Loader -->
	    <div id="loader-wrapper">
	        <div id="loader"></div>
	        <div class="loader-section section-left"></div>
	        <div class="loader-section section-right"></div>
	    </div>

	    <!-- Navigation Bar -->
	    <nav class="navbar navbar-fixed-top">
	        <div class="navbar-header">
				<div class="navbar-brand">
					<a class="navbar-logo">
						<img src="../images/common/gff-logo.svg" class="logo-full" alt="" />
						<img src="../images/common/gff-logo-s.svg" class="logo-mobile" alt="" />
					</a>
				</div>

				<button type="button" class="navbar-toggle collapsed category-button" aria-expanded="false">
					<i class="fa fa-bars"></i>
				</button>

				<button type="button" class="navbar-toggle collapsed user-button" data-toggle="collapse" data-target="#nav-menu" aria-expanded="false">
					<i class="fa fa-user"></i>
				</button>

				<button type="button" class="navbar-toggle collapsed search-button" data-toggle="collapse" data-target="#nav-search" aria-expanded="false">
					<i class="fa fa-search"></i>
				</button>
			</div>

			<div class="collapse navbar-collapse" id="nav-search">
				<form class="navbar-form">
					<div class="form-group">
						<div class="input-group">
							<input type="text" placeholder="Search an item.." class="form-control navbar-search">
							<span class="input-group-btn">
								<button type="submit" class="btn btn-sm">
									<i class="fa fa-chevron-right"></i>
								</button>
							</span>
						</div>
					</div>
				</form>
			</div>

			<div class="collapse navbar-collapse" id="nav-menu">

				<ul class="nav navbar-nav navbar-right">
					<% if(loggedIn) { %>
						<li id="nav-profile">
			                <a href="/profile/<%= id %>">
								<div class="user-avatar">
									<img src="../images/common/test-user.png" alt="" />
									<% if (typeof notification != 'undefined' ) { %>
										<span class="badge"><%= notification.length %></span>
									<% } %>
									<span> Patrick</span>
								</div>
			                </a>
			            </li>
						<li id="nav-create">
			                <a href="/upload">
			                    <i class="fa fa-gift" aria-hidden="true"></i> Give
			                </a>
			            </li>
						<li id="nav-feed">
			                <a href="/">
			                    <i class="fa fa-newspaper-o" aria-hidden="true"></i> Feed
			                </a>
			            </li>
						<li id="nav-logout">
							<a href="/logout">Log out</a>
						</li>
					<%  } else { %>
						<li id="nav-feed">
			                <a href="/">
			                    <i class="fa fa-newspaper-o" aria-hidden="true"></i> Feed
			                </a>
			            </li>
						<li id="nav-logout">
							<a href="javascript:loginSpecial();">Log in</a>
						</li>
					<% } %>
				</ul>
	        </div>
	        <!-- Menu -->

	    </nav>

		<div class="col-sm-3 floating-bar-mobile">
			<nav class="category-bar">
				<ul class="nav">
					<li class="category-bar-title">Categories</li>
					<li class="category-bar-item"><a href="#">Accessories <i class="fa fa-angle-right pull-right"></i></a></li>
					<li class="category-bar-item"><a href="#">Clothes <i class="fa fa-angle-right pull-right"></i></a></li>
